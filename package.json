{
	"name": "@ericgibby/angular-foundation",
	"version": "0.3.0",
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com:ericgibby/angular-foundation.git"
	},
	"author": "Eric Gibby",
	"license": "MIT",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "npm-run-all build:library --parallel build:library:*",
		"build:library": "ng build",
		"build:library:assets": "cpx \"projects/angular-foundation/src/assets/**/*\" \"dist/angular-foundation/assets\"",
		"build:library:css": "webpack --config ./webpack.config.js && shx rm -f \"dist/angular-foundation/main.js\" \"dist/angular-foundation/main.js.map\"",
		"build:library:sass": "cpx \"projects/angular-foundation/src/scss/**/*\" \"dist/angular-foundation/scss\"",
		"prepackage": "npm run build",
		"package": "cd dist/angular-foundation && npm pack",
		"test": "ng test --project=angular-foundation",
		"test:demo": "ng test --project=angular-foundation-demo",
		"lint": "ng lint",
		"e2e": "ng e2e"
	},
	"dependencies": {
		"@angular/animations": "^8.2.8",
		"@angular/common": "^8.2.8",
		"@angular/compiler": "^8.2.8",
		"@angular/core": "^8.2.8",
		"@angular/elements": "^8.2.8",
		"@angular/forms": "^8.2.8",
		"@angular/platform-browser": "^8.2.8",
		"@angular/platform-browser-dynamic": "^8.2.8",
		"@angular/router": "^8.2.8",
		"document-register-element": "^1.7.2",
		"foundation-sites": "^6.5.3",
		"lodash": "^4.17.15",
		"rxjs": "~6.4.0",
		"tslib": "^1.10.0",
		"zone.js": "~0.9.1"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.803.6",
		"@angular-devkit/build-ng-packagr": "~0.803.6",
		"@angular/cli": "~8.3.6",
		"@angular/compiler-cli": "~8.2.8",
		"@angular/language-service": "~8.2.8",
		"@types/jasmine": "~3.3.8",
		"@types/jasminewd2": "~2.0.3",
		"@types/lodash": "^4.14.141",
		"@types/node": "~8.9.4",
		"codelyzer": "^5.0.0",
		"cpx": "^1.5.0",
		"css-loader": "^3.2.0",
		"husky": "^3.0.5",
		"jasmine-core": "~3.4.0",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "~4.1.0",
		"karma-chrome-launcher": "~2.2.0",
		"karma-coverage-istanbul-reporter": "~2.0.1",
		"karma-jasmine": "~2.0.1",
		"karma-jasmine-html-reporter": "^1.4.0",
		"karma-spec-reporter": "0.0.32",
		"mini-css-extract-plugin": "^0.8.0",
		"ng-packagr": "^5.4.0",
		"node-sass": "^7.0.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^1.18.2",
		"pretty-quick": "^1.11.1",
		"protractor": "~5.4.0",
		"sass-loader": "^8.0.0",
		"shx": "^0.3.2",
		"ts-node": "~7.0.0",
		"tsickle": "^0.37.0",
		"tslint": "~5.15.0",
		"typescript": "~3.5.3",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.9"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	}
}
